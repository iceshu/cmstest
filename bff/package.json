    "archiver": "^5.0.2",
    "dayjs": "^1.8.35",
    "egg-bcrypt": "^1.1.0",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.2.0",
    "egg-scripts": "^2.10.0",
    "egg-validate-plus": "^1.1.6",
    "koa-bodyparser": "^4.3.0",
    "midway": "^1.0.0",
    "muri": "^1.3.0",
    "mz-modules": "^2.1.0",
    "node-qiniu": "^6.2.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.5.5",
    "@waiting/eslint-config": "^2.1.0",
    "cross-env": "^6.0.0",
    "egg-ci": "^1.8.0",
    "eslint": "^6.0.0",
    "midway-bin": "1",
    "midway-mock": "1",
    "mocha": "^6.2.0",
    "tslib": "^1.8.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.0"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "build": "midway-bin build -c",
    "ci": "npm run cov",
    "clean": "midway-bin clean",
    "cov": "midway-bin cov --ts",
    "debug": "cross-env NODE_ENV=local midway-bin debug --ts",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "devtest": "cross-env TS_NODE_PROJECT=test/tsconfig.json midway-bin test --ts",
    "lint": "eslint --fix {src,test}/**/*.ts",
    "start": "egg-scripts start --daemon --title=egg-server-bff --framework=midway --ts",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "stop": "egg-scripts stop --title=egg-server-bff",
    "test": "npm run lint && npm run devtest",
    "autod": "midway-bin autod"
  },
  "ci": {
    "version": "10, 12"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-bin-build": {
    "include": [
      "app/public",
      "app/view"
    ]
  },
  "repository": {
    "type": "git",
